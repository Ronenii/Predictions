<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<GridPane minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <columnConstraints>
      <ColumnConstraints hgrow="NEVER" minWidth="10.0" prefWidth="10.0" />
      <ColumnConstraints hgrow="NEVER" maxWidth="174.0" minWidth="150.0" prefWidth="171.0" />
      <ColumnConstraints hgrow="NEVER" maxWidth="10.0" minWidth="10.0" prefWidth="10.0" />
      <ColumnConstraints hgrow="ALWAYS" maxWidth="700.0" minWidth="400.0" prefWidth="480.0" />
      <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="10.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="10.0" vgrow="NEVER" />
      <RowConstraints minHeight="150.0" prefHeight="200.0" vgrow="NEVER" />
      <RowConstraints maxHeight="100.0" minHeight="-Infinity" prefHeight="10.0" vgrow="ALWAYS" />
      <RowConstraints minHeight="10.0" prefHeight="10.0" vgrow="NEVER" />
      <RowConstraints minHeight="10.0" vgrow="NEVER" />
      <RowConstraints minHeight="10.0" prefHeight="10.0" vgrow="NEVER" />
   </rowConstraints>
   <children>
      <TableView prefHeight="200.0" GridPane.columnIndex="3" GridPane.rowIndex="1" GridPane.rowSpan="2">
         <columns>
            <TableColumn prefWidth="75.0" text="C1" />
            <TableColumn prefWidth="75.0" text="C2" />
         </columns>
      </TableView>
      <AnchorPane GridPane.columnIndex="3" GridPane.rowIndex="4">
         <children>
            <fx:include source="Control%20Bar.fxml" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
         </children>
      </AnchorPane>
      <VBox prefHeight="200.0" prefWidth="100.0" spacing="13.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
         <children>
            <Label text="Execution Details" textAlignment="CENTER">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Label>
            <HBox spacing="13.0">
               <children>
                  <Label text="Simulation ID:" />
                  <Label id="lbl-ExecDetails-SimID" text="-" />
               </children>
            </HBox>
            <HBox spacing="13.0">
               <children>
                  <Label text="Current Tick" />
                  <Label id="lbl-ExecDetails-CurrTick" text="-" />
               </children>
            </HBox>
            <HBox spacing="13.0">
               <children>
                  <Label text="Duration" />
                  <Label id="lbl-ExecDetails-Duration" text="-" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</GridPane>
